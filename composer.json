{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-decimal": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "async-aws/ses": "^1.4",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "dunglas/doctrine-json-odm": "^1.1",
        "erusev/parsedown": "^1.7",
        "espresso-dev/instagram-basic-display-php": "^1.1",
        "facebook/graph-sdk": "^5.7",
        "friendsofsymfony/rest-bundle": "^3.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-bundle": "^1.5",
        "lexik/jwt-authentication-bundle": "^2.9",
        "martin-georgiev/postgresql-for-doctrine": "^1.5",
        "nelmio/api-doc-bundle": "^4.0",
        "nelmio/cors-bundle": "^2.1",
        "oneup/flysystem-bundle": "^3.5",
        "pagarme/pagarme-php": "^4.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "raiym/instagram-php-scraper": "^0.10.1",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.6",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/amazon-mailer": "5.2.*",
        "symfony/amazon-sqs-messenger": "5.2.*",
        "symfony/amqp-messenger": "5.2.*",
        "symfony/asset": "5.2.*",
        "symfony/browser-kit": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/options-resolver": "5.2.*",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/markdown-extra": "^3.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/maker-bundle": "^1.23"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
