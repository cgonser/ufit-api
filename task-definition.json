{
  "family": "api",
  "executionRoleArn": "arn:aws:iam::590392040163:role/ecs-task-execution-role",
  "containerDefinitions": [
    {
      "essential": true,
      "memoryReservation": 64,
      "name": "php-fpm",
      "image": "",
      "environment": [],
      "secrets": [
        {
          "valueFrom": "arn:aws:ssm:eu-central-1:590392040163:parameter/production/database/url",
          "name": "DATABASE_URL"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "prod/ecs/api",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "php-fpm"
        }
      }
    },
    {
      "essential": true,
      "memoryReservation": 32,
      "name": "nginx",
      "image": "nginx:1-alpine",
      "environment": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "prod/ecs/api",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "nginx"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "volumesFrom": [
        {
          "sourceContainer": "php-fpm"
        }
      ],
      "dependsOn": [
        {
          "containerName": "php-fpm",
          "condition": "START"
        }
      ],
      "links": [
        "php-fpm"
      ]
    }
  ]
}